{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PricingServiceConnection": "THIS IS SECRET"
  },
  "spring": {
    "application": { "name": "PricingService" },
    "cloud": {
      "config": {
        "uri": "http://localhost:5005",
        "validate_certificates": false
      }
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": "http://localhost:8761/eureka",
      "ValidateCertificates": false
    },
    "instance": {
      "appName": "PricingService",
      "port": "5005",
      "hostName": "localhost",
      "healthCheckUrlPath": "/health",
      "statusPageUrlPath": "/health",
      "instanceId": "localhost:5005"
    }
  }
}